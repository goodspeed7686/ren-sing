<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd"><sqlMap namespace="ren.sing.dao.table">	<insert id="class_place_insert" parameterClass="java.util.HashMap">		<![CDATA[			INSERT INTO CLASS_PLACE (				class_place_id,				name				) VALUES (				#class_place_id#,				#name#			)		]]>	</insert>	<select id="class_place_query" resultClass="java.util.HashMap" parameterClass="java.util.HashMap">		<![CDATA[			SELECT class_place_id,name			FROM CLASS_PLACE		]]>		<dynamic prepend="WHERE">			<isNotEmpty prepend="AND" property="class_place_id">				<![CDATA[ class_place_id = #class_place_id#  ]]>			</isNotEmpty>			<isNotEmpty prepend="AND" property="name">				<![CDATA[ name = #name#  ]]>			</isNotEmpty>		</dynamic>		<isNotEmpty property="order">			<![CDATA[	ORDER BY $order$	]]>		</isNotEmpty>		<isNotEmpty prepend="LIMIT" property="rowLimit">			<isNotEmpty property="fromIndex">				<![CDATA[ $fromIndex$, $rowLimit$ ]]>			</isNotEmpty>			<isEmpty property="fromIndex">				<![CDATA[ $rowLimit$ ]]>			</isEmpty>		</isNotEmpty>	<!--  	<include refid="limit_offset" /> -->	</select>	<update id="class_place_update" parameterClass="java.util.HashMap">		<![CDATA[			UPDATE CLASS_PLACE		]]>		<dynamic prepend="SET">			<isNotEmpty prepend="," property="class_place_id">				<![CDATA[ class_place_id = #class_place_id#  ]]>			</isNotEmpty>			<isNotEmpty prepend="," property="name">				<![CDATA[ name = #name#  ]]>			</isNotEmpty>		</dynamic>		<![CDATA[			WHERE				class_place_id = #class_place_id#		]]>	</update>	<delete id="class_place_delete" parameterClass="java.util.HashMap">		<![CDATA[			DELETE FROM CLASS_PLACE WHERE class_place_id = #class_place_id#		]]>	</delete>	<insert id="class_place_upsert" parameterClass="java.util.HashMap">		<![CDATA[			INSERT INTO CLASS_PLACE (				class_place_id,				name				) VALUES (				#class_place_id#,				#name#			)			ON DUPLICATE KEY UPDATE				class_place_id = #class_place_id#,				name = #name#		]]>	</insert></sqlMap>