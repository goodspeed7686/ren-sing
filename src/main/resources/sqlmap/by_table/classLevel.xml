<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd"><sqlMap namespace="ren.sing.dao.table">	<insert id="class_level_insert" parameterClass="java.util.HashMap">		<![CDATA[			INSERT INTO CLASS_LEVEL (				class_level_id,				level,				name				) VALUES (				#class_level_id#,				#level#,				#name#			)		]]>	</insert>	<select id="class_level_query" resultClass="java.util.HashMap" parameterClass="java.util.HashMap">		<![CDATA[			SELECT class_level_id,level,name			FROM CLASS_LEVEL		]]>		<dynamic prepend="WHERE">			<isNotEmpty prepend="AND" property="class_level_id">				<![CDATA[ class_level_id = #class_level_id#  ]]>			</isNotEmpty>			<isNotEmpty prepend="AND" property="level">				<![CDATA[ level = #level#  ]]>			</isNotEmpty>			<isNotEmpty prepend="AND" property="name">				<![CDATA[ name = #name#  ]]>			</isNotEmpty>		</dynamic>		<isNotEmpty property="order">			<![CDATA[	ORDER BY $order$	]]>		</isNotEmpty>		<isNotEmpty prepend="LIMIT" property="rowLimit">			<isNotEmpty property="fromIndex">				<![CDATA[ $fromIndex$, $rowLimit$ ]]>			</isNotEmpty>			<isEmpty property="fromIndex">				<![CDATA[ $rowLimit$ ]]>			</isEmpty>		</isNotEmpty>	<!--  	<include refid="limit_offset" /> -->	</select>	<update id="class_level_update" parameterClass="java.util.HashMap">		<![CDATA[			UPDATE CLASS_LEVEL		]]>		<dynamic prepend="SET">			<isNotEmpty prepend="," property="class_level_id">				<![CDATA[ class_level_id = #class_level_id#  ]]>			</isNotEmpty>			<isNotEmpty prepend="," property="level">				<![CDATA[ level = #level#  ]]>			</isNotEmpty>			<isNotEmpty prepend="," property="name">				<![CDATA[ name = #name#  ]]>			</isNotEmpty>		</dynamic>		<![CDATA[			WHERE				class_level_id = #class_level_id#		]]>	</update>	<delete id="class_level_delete" parameterClass="java.util.HashMap">		<![CDATA[			DELETE FROM CLASS_LEVEL WHERE class_level_id = #class_level_id#		]]>	</delete>	<insert id="class_level_upsert" parameterClass="java.util.HashMap">		<![CDATA[			INSERT INTO CLASS_LEVEL (				class_level_id,				level,				name				) VALUES (				#class_level_id#,				#level#,				#name#			)			ON DUPLICATE KEY UPDATE				class_level_id = #class_level_id#,				level = #level#,				name = #name#		]]>	</insert></sqlMap>