<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd"><sqlMap namespace="ren.sing.dao.table">	<insert id="courses_time_insert" parameterClass="java.util.HashMap">		<![CDATA[			INSERT INTO COURSES_TIME (				id,				interval_time,				start_time,				end_time				) VALUES (				#id#,				#interval_time#,				#start_time#,				#end_time#			)		]]>	</insert>	<select id="courses_time_query" resultClass="java.util.HashMap" parameterClass="java.util.HashMap">		<![CDATA[			SELECT id,interval_time,start_time,end_time			FROM COURSES_TIME		]]>		<dynamic prepend="WHERE">			<isNotEmpty prepend="AND" property="id">				<![CDATA[ id = #id#  ]]>			</isNotEmpty>			<isNotEmpty prepend="AND" property="interval_time">				<![CDATA[ interval_time = #interval_time#  ]]>			</isNotEmpty>			<isNotEmpty prepend="AND" property="start_time">				<![CDATA[ start_time = #start_time#  ]]>			</isNotEmpty>			<isNotEmpty prepend="AND" property="end_time">				<![CDATA[ end_time = #end_time#  ]]>			</isNotEmpty>		</dynamic>		<isNotEmpty property="order">			<![CDATA[	ORDER BY $order$	]]>		</isNotEmpty>		<isNotEmpty prepend="LIMIT" property="rowLimit">			<isNotEmpty property="fromIndex">				<![CDATA[ $fromIndex$, $rowLimit$ ]]>			</isNotEmpty>			<isEmpty property="fromIndex">				<![CDATA[ $rowLimit$ ]]>			</isEmpty>		</isNotEmpty>	<!--  	<include refid="limit_offset" /> -->	</select>	<update id="courses_time_update" parameterClass="java.util.HashMap">		<![CDATA[			UPDATE COURSES_TIME		]]>		<dynamic prepend="SET">			<isNotEmpty prepend="," property="id">				<![CDATA[ id = #id#  ]]>			</isNotEmpty>			<isNotEmpty prepend="," property="interval_time">				<![CDATA[ interval_time = #interval_time#  ]]>			</isNotEmpty>			<isNotEmpty prepend="," property="start_time">				<![CDATA[ start_time = #start_time#  ]]>			</isNotEmpty>			<isNotEmpty prepend="," property="end_time">				<![CDATA[ end_time = #end_time#  ]]>			</isNotEmpty>		</dynamic>		<![CDATA[			WHERE				id = #id#		]]>	</update>	<delete id="courses_time_delete" parameterClass="java.util.HashMap">		<![CDATA[			DELETE FROM COURSES_TIME WHERE id = #id#		]]>	</delete>	<insert id="courses_time_upsert" parameterClass="java.util.HashMap">		<![CDATA[			INSERT INTO COURSES_TIME (				id,				interval_time,				start_time,				end_time				) VALUES (				#id#,				#interval_time#,				#start_time#,				#end_time#			)			ON DUPLICATE KEY UPDATE				id = #id#,				interval_time = #interval_time#,				start_time = #start_time#,				end_time = #end_time#		]]>	</insert></sqlMap>